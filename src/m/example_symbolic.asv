x = sym('x');
y = sym('y');

2*x + 3*x - y    %algebraic computation

f = x^2;
diff(f,x)        %differentiates x^2 w.r.t. x

f = cos(x);
int(f,x)         %integrates cos(x) w.r.t. x

f = (x^2 + x^3)/x^2;
simplify(f)      %simplifies f



f = 15*x - 15;
solve(f,x)       %solves 15*x - 15 = 0